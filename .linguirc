{
  "catalogs": [
    {
      "path": "<rootDir>/apps/sjfl-judians/src/locales/{locale}/messages",
      "include": [
        "<rootDir>"
      ],
      "exclude": [
        "**/node_modules/**",
        "<rootDir>/apps/sjfl-judians-e2e/**"
      ]
    }
  ],
  "compileNamespace": "ts",
  "fallbackLocales": {
    "default": "en"
  },
  "format": "po",
  "locales": [
    "en",
    "hi",
    "mr",
    "bn",
    "kn",
    "ta",
    "te",
    "ml"
  ],
  "orderBy": "messageId",
  "pseudoLocale": "",
  "rootDir": ".",
  "runtimeConfigModule": [
    "@lingui/core",
    "i18n"
  ],
  "sourceLocale": "en",
  "service": {
    "name": "TranslationIO",
    "apiKey": "c7f8be8c88bc424fbe52afd565ad368d"
  }
}
