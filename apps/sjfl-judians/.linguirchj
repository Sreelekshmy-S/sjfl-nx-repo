{
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src"
      ],
      "exclude": [
        "**/node_modules/**"
      ]
    }
  ],
  "compileNamespace": "cjs",
  "extractBabelOptions": {
    "rootMode": "upward"
  },
  "fallbackLocales": {},
  "format": "po",
  "locales": [
    "en",
    "hi"
  ],
  "orderBy": "messageId",
  "pseudoLocale": "",
  "rootDir": ".",
  "runtimeConfigModule": [
    "@lingui/core",
    "i18n"
  ],
  "sourceLocale": ""
}
